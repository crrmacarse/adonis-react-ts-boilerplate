{
  "name": "adonis-react-ts-boilerplate",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "The fullstack application boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "hot": "webpack-dev-server --hot --open --profile --progress --colors --config=webpack.config.babel.js --env=hot",
    "dev": "webpack --hot --profile --progress --config=webpack.config.babel.js --env=development",
    "prod": "webpack --profile --progress --config=webpack.config.babel.js --env=production",
    "ssr": "webpack --profile --progress --config=webpack.config.babel.js --env=server",
    "build:stats": "webpack --profile --json --config=webpack.config.babel.js --env=production > compilation-stats.json",
    "analyze": "webpack-bundle-analyzer compilation-stats.json public",
    "lint": "eslint --ext .js,.ts,.tsx,*.test.tsx . --ignore-pattern node_modules/ --ignore-pattern public/",
    "test": "node ace test",
    "test-frontend": "jest --verbose --colors --coverage",
    "test-frontend:watch": "jest --colors --watch",
    "serve-frontend": "serve public",
    "start": "node server.js"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "@crrmacarse",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.27",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/vow": "^1.0.17",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "final-form": "^4.18.7",
    "history": "^4.10.1",
    "pem": "^1.14.4",
    "raf": "^3.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-final-form": "^6.5.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.3",
    "serve": "^11.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^5.0.2",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.1.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "autoload": {
    "App": "./app"
  }
}
